{

  "manifest_version": 2,
  "name": "Wikipedia.random New Tab",
  "homepage_url": "https://github.com/daiyitastic/random-wikipedia-newtab-extension",
  "version": "1.0",

  "description": "Redirects to a random wikipedia page when you open a new tab",

  "icons": {
    "48": "icons/random-new-tab-48.gif"
  },

  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],

  "background": {
    "scripts": ["randomNewTab.js"]
  },

  "options_ui": {
    "page": "options/settings.html",
    "open_in_tab": true
  }

}
