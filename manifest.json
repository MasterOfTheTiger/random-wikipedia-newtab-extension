{
  "manifest_version": 2,
  "name": "Random unusual Wikipedia new tab",
  "description": "Redirects to a random unusual wikipedia page when you open a new tab",
  "homepage_url": "https://github.com/daiyitastic/random-wikipedia-newtab-extension",
  "version": "1.0",
  "icons": {
    "48": "icons/random-new-tab-48.png"
  },
  "permissions": [
    "<all_urls>",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": ["randomNewTab.js"]
  },
  "options_ui": {
    "page": "options/settings.html",
    "open_in_tab": true
  }
}
